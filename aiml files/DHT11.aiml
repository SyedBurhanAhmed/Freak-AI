<category>
    <pattern>WHAT IS THE HUMIDITY LEVEL</pattern>
    <template>
        <think>
            <set name="get_dht11_humidity">true</set>
            <set name="analyze_dht11_environment">true</set>
        </think>
        <condition name="dht11_humidity">
            <li value="unavailable">I can't access the humidity sensor right now.</li>
            <li>
                The humidity level is <get name="dht11_humidity"/>%.
                <condition name="dht11_humidity">
                    <li value="*">
                        <javascript>
                            var humidity = parseFloat('<get name="dht11_humidity"/>');
                            var level = "";

                            if (humidity >= 70) level = "This is very high humidity - extremely humid conditions!";
                            else if (humidity >= 60) level = "This is high humidity - quite humid.";
                            else if (humidity >= 50) level = "This is moderate humidity - comfortable level.";
                            else if (humidity >= 40) level = "This is normal humidity - good level.";
                            else if (humidity >= 30) level = "This is low humidity - getting dry.";
                            else level = "This is very low humidity - extremely dry conditions!";

                            return " " + level;
                        </javascript>
                    </li>
                </condition>
            </li>
        </condition>
    </template>
</category>

