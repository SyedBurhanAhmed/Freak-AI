        <think>
            <set name="get_dht11_temperature">true</set>
            <set name="get_dht11_humidity">true</set>
            <set name="analyze_dht11_environment">true</set>
        </think>
        <condition name="dht11_temperature">
            <li value="unavailable">I can't access the DHT11 environmental sensors right now.</li>
            <li>
                Based on my DHT11 sensor readings: Temperature is <get name="dht11_temperature"/>°C and humidity is <get name="dht11_humidity"/>%.
                Your comfort score is <get name="dht11_comfort_score"/>%.
                <condition name="dht11_recommendations">
