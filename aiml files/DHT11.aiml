
<aiml version="2.0">

<category>
    <pattern>WHAT IS THE TEMPERATURE</pattern>
    <template>
        <think><set name="get_dht11_temperature">true</set></think>
        <condition name="dht11_temperature">
            <li value="unavailable">I can't access the DHT11 temperature sensor right now.</li>
            <li>The DHT11 sensor shows the current temperature is <get name="dht11_temperature"/>°C.</li>
        </condition>
    </template>
</category>

