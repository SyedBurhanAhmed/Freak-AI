<category>
    <pattern>WHAT ARE THE ENVIRONMENTAL LEVELS</pattern>
    <template>
        <think>
            <set name="get_dht11_temperature">true</set>
            <set name="get_dht11_humidity">true</set>
            <set name="analyze_dht11_environment">true</set>
        </think>
        <condition name="dht11_temperature">
            <li value="unavailable">I can't access the environmental sensors right now.</li>
            <li>
                Here are the current environmental levels:

                Temperature Level: <get name="dht11_temperature"/>°C
                <condition name="dht11_temperature">
                    <li value="*">
                        <javascript>
                        <![CDATA[
                            var temp = parseFloat('<get name="dht11_temperature"/>');
                            if (temp >= 30) return "- Hot level";
                            else if (temp >= 20) return "- Comfortable level";
                            else return "- Cold level";
                        ]]>
                        </javascript>
                    </li>
                </condition>

                Humidity Level: <get name="dht11_humidity"/>%
                <condition name="dht11_humidity">
                    <li value="*">
                        <javascript>
                        <![CDATA[
                            var humidity = parseFloat('<get name="dht11_humidity"/>');
                            if (humidity >= 60) return "- High humidity level";
                            else if (humidity >= 40) return "- Normal humidity level";
                            else return "- Low humidity level";
                        ]]>
                        </javascript>
                    </li>
                </condition>

                Comfort Score: <get name="dht11_comfort_score"/>%
                <condition name="dht11_recommendations">
                    <li value="">Overall: Optimal environmental conditions!</li>
                    <li>Recommendations: <get name="dht11_recommendations"/></li>
                </condition>
            </li>
        </condition>
    </template>
</category>

<category>
    <pattern>WHAT IS MY COMFORT LEVEL</pattern>
    <template>
        <think><set name="analyze_dht11_environment">true</set></think>
        <condition name="dht11_comfort_score">
            <li value="">I need to check the environmental sensors to determine your comfort level.</li>
            <li>
                Your comfort level is <get name="dht11_comfort_score"/>% based on the current environmental conditions.
                <condition name="dht11_comfort_score">
                    <li value="*">
                        <javascript>
                        <![CDATA[
                            var score = parseFloat('<get name="dht11_comfort_score"/>');
                            var comfort = "";

                            if (score >= 80) comfort = "Excellent - You should feel very comfortable!";
                            else if (score >= 60) comfort = "Good - Reasonably comfortable conditions.";
                            else if (score >= 40) comfort = "Fair - Some discomfort may be present.";
                            else if (score >= 20) comfort = "Poor - Uncomfortable conditions.";
                            else comfort = "Very Poor - Highly uncomfortable environment!";

                            return " " + comfort;
                        ]]>
                        </javascript>
                    </li>
                </condition>
            </li>
        </condition>
    </template>
</category>

