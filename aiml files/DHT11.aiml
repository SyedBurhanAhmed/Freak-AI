<category>
    <pattern>ENVIRONMENTAL STATUS</pattern>
    <template>
        <think>
            <set name="get_dht11_temperature">true</set>
            <set name="get_dht11_humidity">true</set>
            <set name="analyze_dht11_environment">true</set>
        </think>
        <condition name="dht11_temperature">
            <li value="unavailable">Environmental sensors are not accessible.</li>
            <li>
                Current Environmental Status:

                [TEMP] Temperature: <get name="dht11_temperature"/>°C
                <condition name="dht11_temperature">
                    <li value="*">
                        <javascript>
                        <![CDATA[
                            var temp = parseFloat('<get name="dht11_temperature"/>');
                            if (temp >= 30) return "(Hot)";
                            else if (temp >= 20) return "(Normal)";
                            else return "(Cold)";
                        ]]>
                        </javascript>
                    </li>
                </condition>

                [HUM] Humidity: <get name="dht11_humidity"/>%
                <condition name="dht11_humidity">
                    <li value="*">
                        <javascript>
                        <![CDATA[
                            var humidity = parseFloat('<get name="dht11_humidity"/>');
                            if (humidity >= 60) return "(Humid)";
                            else if (humidity >= 40) return "(Normal)";
                            else return "(Dry)";
                        ]]>
                        </javascript>
                    </li>
                </condition>

                [COMFORT] Score: <get name="dht11_comfort_score"/>%

                <condition name="dht11_recommendations">
                    <li value="">[OK] Environment is optimal!</li>
                    <li>[INFO] Suggestions: <get name="dht11_recommendations"/></li>
                </condition>
            </li>
        </condition>
    </template>
</category>

